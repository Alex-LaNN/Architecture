<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
  <form class="navbar-form navbar-right">
    <div class="form-group">
      <input id="search" type="text" placeholder="Найди для себя интересную книгу (по названию, по имени автора или его id, по году выпуска)..." class="form-control">
        <script>
          $("#search").bind("keypress", function (e) {
            let searchString = $("#search").val().trim();
            if (e.keyCode == 13) {
              e.preventDefault();
                if (searchString) {
                  window.location.href = '/search' + '?search=' + encodeURIComponent(searchString);
                $("#searchString").val(searchString);
                document.forms["searchForm"].submit();
              }
            }
          })
        </script>
        <div class="loader"><img src="/images/loading.gif"></div>
        <div id="list" size="" class="bAutoComplete mSearchAutoComplete"></div>
    </div>
  </form>
</div>